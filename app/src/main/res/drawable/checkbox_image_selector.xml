<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Checked → image icon -->
    <item android:state_checked="true"
        android:drawable="@drawable/outline_check_box_checked_24"/>

    <!-- Unchecked → normal checkbox -->
    <item android:state_checked="false"
        android:drawable="@drawable/outline_check_box_unchecked_24"/>

</selector>